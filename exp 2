#include <stdio.h> 
#include <ctype.h> 
int isValidString(const char *str) { 
int i = 0; 
if (str[0] == '\0') return 0; 
while (str[i] != '\0') { 
if (!isalnum(str[i])) return 0; 
i++; 
} 
return 1; 
} 
int main() { 
char str[100]; 
printf("Enter a string: "); 
scanf("%99s", str); 
if (isValidString(str)) 
printf("Valid string.\n"); 
else 
printf("Invalid string.\n"); 
return 0; 
} 
