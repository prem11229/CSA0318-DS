#include <stdio.h> 
#include <stdlib.h> 
struct Node { 
int data; 
struct Node *next; 
}; 
struct Node *head = NULL; 
void insert(int data) { 
struct Node *newNode = (struct Node*)malloc(sizeof(struct Node)); 
newNode->data = data; 
newNode->next = head; 
head = newNode; 
} 
void reverse() { 
struct Node *prev = NULL, *curr = head, *next = NULL; 
while (curr) { 
next = curr->next; 
curr->next = prev; 
prev = curr; 
curr = next; 
} 
head = prev; 
} 
void display() { 
struct Node *temp = head; 
while (temp) { 
printf("%d -> ", temp->data); 
temp = temp->next; 
} 
printf("NULL\n"); 
} 
int main() { 
insert(30); 
insert(20); 
insert(10); 
printf("Original List:\n"); 
display(); 
reverse(); 
printf("Reversed List:\n"); 
display(); 
return 0; 
}
